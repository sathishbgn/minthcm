# MintHCM environment variables (copy to .env and edit for local use)
# Do NOT commit your .env with secrets; keep this file as a template

# Web server port forwarded from container
WEB_PORT=8080

# MySQL root / admin password used by containers and installer
MYSQL_PASSWORD=secret
MYSQL_ROOT_PASSWORD=secret

# Optional: external Elasticsearch host (if not using container)
ELASTICSEARCH_HOST=http://localhost:9200

# PHP / application env (dev|prod)
APP_ENV=dev
